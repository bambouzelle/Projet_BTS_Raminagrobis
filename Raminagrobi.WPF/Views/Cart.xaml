<UserControl x:Class="Raminagrobis.WPF.Views.Cart"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Raminagrobis.WPF.Views"
             xmlns:viewModels="clr-namespace:Raminagrobis.WPF.ViewModels"
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance viewModels:CartViewModel}"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="Yellow">
        <DataGrid ItemsSource="{Binding CartList}"/>
    </Grid>
</UserControl>
